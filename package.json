{
  "name": "civic-platform",
  "version": "0.1.0",
  "description": "Civic - A Hack Oregon Project",
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "contributors": [
    "David Daniel <davidedaniel@gmail.com>"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./build",
    "dev": "babel-node tools/dev",
    "build": "webpack --config tools/webpack.config.babel.js --colors",
    "build:server": "babel src --out-dir build --copy-files",
    "build:prod": "NODE_ENV=production run-s build build:server",
    "lint": "eslint src",
    "start": "NODE_ENV=production npm run go",
    "serve:app": "node build/server/index.js",
    "serve:static": "node staticServer/index.js",
    "preserve:server": "npm run build:server",
    "prego": "BABEL_ENV=production run-s clean build build:server",
    "go": "BABEL_ENV=production run-p serve:static serve:app",
    "test": "mocha src/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hackoregon/civic-platform"
  },
  "dependencies": {
    "@hackoregon/civic-server": "0.0.10",
    "@hackoregon/component-library": "^0.2.5",
    "app-root-dir": "^1.0.2",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "isomorphic-loader": "^1.6.2",
    "npm-run-all": "^4.0.2",
    "pm2": "^2.4.6",
    "postcss-loader": "^1.3.3",
    "ramda": "^0.23.0",
    "react": "15.5.3",
    "react-dom": "15.5.3",
    "react-helmet": "^5.0.2",
    "react-redux": "^5.0.3",
    "react-router": "3.0.3",
    "react-router-dom": "^4.0.0-beta.6",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "reselect": "^3.0.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-partial": "^2.0.2"
  },
  "devDependencies": {
    "@hackoregon/webpacker": "^0.1.7",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0-beta.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.23.0",
    "colors": "^1.1.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flowtype": "2.29.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "^6.10.0",
    "mocha": "^3.2.0",
    "node-notifier": "^5.0.2",
    "regenerator-runtime": "^0.10.1",
    "rimraf": "^2.5.4",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0",
    "webpack-node-externals": "^1.5.4"
  }
}
