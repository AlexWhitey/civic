{
  "extends": "airbnb",
  "plugins": ["react"],
  "rules": {
    "new-cap": 0,
    "func-names": 2,
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0
  },
  "globals": {
    "__DEV__": true,
    "window": true,
    "document": true,
    "describe": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "afterEach": true,
    "before": true,
    "after": true
  }
}